{
  "env": {
    "node": true,
    "browser": true,
    "es6": false
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "no-unused-vars": "off"
  }
}
